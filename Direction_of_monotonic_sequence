def order(l):  
   cnti=0
   cntd=0
   for i in range(len(l)-1):
     if(l[i]<l[i+1]):
        cnti+=1
     elif(l[i]>l[i+1]):
         cntd+=1
     else:
       pass
   if(cnti==(len(l)-1)):
         print("increasing")
   elif(cntd==(len(l)-1)):
         print("decreasing")
   else:
    print("neither increasing nor decreasing")
 
      
order([9,4,3,5])
